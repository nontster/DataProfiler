          "queryType": "sql",
          "rawSql": "WITH e1 AS (SELECT DISTINCT table_name, schema_name FROM \"default\".\"table_inventory\" WHERE application = ${application:singlequote} AND environment = ${env1:singlequote} AND scan_time = (SELECT max(scan_time) FROM \"default\".\"table_inventory\" WHERE application = ${application:singlequote} AND environment = ${env1:singlequote})), e2 AS (SELECT DISTINCT table_name, schema_name FROM \"default\".\"table_inventory\" WHERE application = ${application:singlequote} AND environment = ${env2:singlequote} AND scan_time = (SELECT max(scan_time) FROM \"default\".\"table_inventory\" WHERE application = ${application:singlequote} AND environment = ${env2:singlequote})) SELECT coalesce(e1.table_name, e2.table_name) as table_name, coalesce(e1.schema_name, e2.schema_name) as schema_name, multiIf(e1.table_name != '' AND e2.table_name != '', '‚úì Both', e1.table_name != '', concat('‚ö† Only in ', ${env1:singlequote}), concat('‚ö† Only in ', ${env2:singlequote})) as status FROM e1 FULL OUTER JOIN e2 ON e1.table_name = e2.table_name ORDER BY multiIf(e1.table_name != '' AND e2.table_name != '', 2, 1), coalesce(e1.table_name, e2.table_name)",
          "refId": "A"
